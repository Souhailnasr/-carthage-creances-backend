							"script": {
								"exec": [
									"if (pm.response.code === 200 || pm.response.code === 201) {",
									"    var jsonData = pm.response.json();",
									"    if (jsonData.id) {",
									"        pm.environment.set(\"dossier_id\", jsonData.id);",
									"        console.log(\"Dossier créé avec ID: \" + jsonData.id);",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "Bearer {{token}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"titre\": \"Test Prédiction IA\",\n  \"description\": \"Dossier de test pour la prédiction IA\",\n  \"numeroDossier\": \"TEST-IA-001\",\n  \"montantCreance\": 50000.0,\n  \"urgence\": \"MOYENNE\",\n  \"dossierStatus\": \"ENCOURSDETRAITEMENT\",\n  \"statut\": \"VALIDE\",\n  \"typeRecouvrement\": \"AMIABLE\",\n  \"typeCreancier\": \"PERSONNE_MORALE\",\n  \"nomCreancier\": \"Entreprise Test\",\n  \"typeDebiteur\": \"PERSONNE_MORALE\",\n  \"nomDebiteur\": \"Débiteur Test\"\n}"
						},
